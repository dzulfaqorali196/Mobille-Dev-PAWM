{
  "expo": {
    "name": "Virton",
    "slug": "virton",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1b2838"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1b2838"
      },
      "package": "com.virton.app",
      "versionCode": 1,
      "permissions": []
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "newArchEnabled": true
    },
    "scheme": "virton",
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
